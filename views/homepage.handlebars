{{#each posts as |post|}}
<div class="card blog-post">
    <div class="card-header blog-post-header">
        {{title}}
        <span class="blog-post-byline">Posted by username on mm/dd/yyyy</span>
    </div>
    <div class="card-body blog-post-text">
        <p class="card-text">{{content}}</p>
    </div>
</div>
{{/each}}